env CC=`which clang` CXX=`which clang++` \
cmake -DLLVM_ENABLE_PROJECTS=clang -DLLVM_USE_NEWPM=ON -fuse-ld=lld -DLLVM_USE_LINKER=lld -DLLVM_TARGETS_TO_BUILD="X86;RISCV" -DBUILD_SHARED_LIBS=ON -DLLVM_OPTIMIZED_TABLEGEN=ON  -DCMAKE_BUILD_TYPE=Debug -DLLVM_USE_SPLIT_DWARF=ON LLVM_PARALLEL_LINK_JOBS=1 ../llvm

make -j4


make check-clang
